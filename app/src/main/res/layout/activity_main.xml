        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Kulit di bokong dan di area selangkangan hingga ke paha hangat bila disentuh" />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="infeksi terjadi di lipatan kulit" />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="benjolan berisi nanah di lipatan kulit" />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ruam yang disertai gatal dan rasa terbakar di lipatan kulit" />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox25"
            android:layout_width="match_parent"
