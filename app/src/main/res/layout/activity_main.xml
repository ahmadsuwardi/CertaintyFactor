        <CheckBox
            android:id="@+id/checkBox10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="gatal disertai sensasi panas dan menyengat di antara jari kaki  " />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Masukan Nilai Kepercayaan" />

        <CheckBox
            android:id="@+id/checkBox11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="gatal disertai sensasi panas dan menyengat di telapak kaki." />

